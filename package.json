  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng run lover-flower:serve-ssr",
    "serve": "node dist/lover-flower/server/main.js",
    "build": "ng build --configuration production --build-optimizer --optimization",
    "build:staging": "ng build --configuration staging --build-optimizer --optimization",
    "build:ssr": "ng build --build-optimizer --optimization && ng run lover-flower:server",
    "prerender": "ng run lover-flower:prerender",
    "watch": "ng build --configuration development --watch",
    "test": "ng test",
    "prepare": "husky install",
    "lint": "ng lint",
    "storybook": "ng run lover-flower:storybook",
    "build-storybook": "ng run lover-flower:build-storybook",
    "typecheck": "tsc --project tsconfig.app.json --noEmit",
    "stylelint": "stylelint 'src/**/*{.scss}'",
    "stylelint:fix": "stylelint 'src/**/*{.scss}' --fix"
  },
